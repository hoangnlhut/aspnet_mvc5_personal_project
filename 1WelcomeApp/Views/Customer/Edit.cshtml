@model _1WelcomeApp.Models.Customer
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<main aria-labelledby="title">
    <h2>Customer Infomation of @Model.Name</h2>
    <h3>Subscribe To Newsletter @Model.IsSubsribedToNewsletter</h3>
    <h3>Sign sup Fee: @Model.MembershipType.SignUpFee</h3>
    <h3>Discount rate: @Model.MembershipType.DiscountRate%</h3>
    <h3>Duration in Month:  @Model.MembershipType.DurationInMonth</h3>
</main>

